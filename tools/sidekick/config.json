{
  "project": "Henkel",
  "plugins": [
    {
      "id": "themeediting",
      "title": "Theme Editing",
      "environments": ["any", "dev", "admin", "edit", "preview"],
      "passConfig": true,
      "passReferrer": true,
      "url": "http://localhost:8888/@henkel/theme-interface/external.html",
      "includePaths": ["**.docx**"]
    },
    {
      "id": "block-library",
      "title": "Block Library",
      "environments": ["edit"],
      "url": "/tools/sidekick/block-library.html",
      "excludePaths": ["**"],
      "includePaths": ["**.docx**", "/document/**"]
    }
  ]
}
